{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "functions": {
    "dist/server.cjs": {
      "runtime": "@vercel/deno",
      "includeFiles": "dist/web/**"
    }
  },
  "routes": [
    { "src": "/api/(.*)", "dest": "/server.cjs" },
    { "src": "/(.*)", "dest": "/server.cjs" }
  ]
}
